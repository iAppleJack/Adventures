<% if !@travel.errors.empty? %>
    <div class = "alert alert-error">

      <ul>
        <% @travel.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
        <% end %>
      </ul>

    </div>
<% end %>

<div class = "well">
  <%= form_for @travel, html: { multipart: true } do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :img %>
  <%= f.file_field :img %>
  <%= f.submit "Save", class: "btn btn-primary" %>
  <% end %>
</div>